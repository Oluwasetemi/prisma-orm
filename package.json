	"devDependencies": {
		"@types/express": "^4.17.11",
		"prisma": "2.22.1",
		"ts-node": "9.1.1",
		"typegraphql-prisma": "^0.14.1",
		"typescript": "4.2.3"
	},
	"scripts": {
		"dev": "ts-node ./script.ts",
		"start:dev": "nodemon --watch \"*\" --ext \"ts,json,graphql,prisma\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node rest-api.ts\"",
