	},
	"dependencies": {
		"@prisma/client": "2.22.1",
		"apollo-server": "^2.24.0",
		"class-validator": "^0.13.1",
		"express": "^4.17.1",
		"graphql": "^15.5.0",
		"graphql-fields": "^2.0.3",
		"reflect-metadata": "^0.1.13",
		"type-graphql": "^1.1.1"
	},
	"engines": {
		"node": ">=10.0.0"
	}
}
